{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/theuy-ui",
  "lib": {
    "entryFile": "src/public-api.ts"
  },
  "assets": [
    {
      "glob": "**/*",
      "input": "src/material",
      "output": "material"
    },
    {
      "glob": "**/*",
      "input": "src/donna",
      "output": "donna"
    }
  ]
}
